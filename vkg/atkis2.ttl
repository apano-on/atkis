#################################################################
#    Classes
#################################################################

###  http://example.org/ontologies/atkis#Administrative
:Administrative rdf:type owl:Class ;
                rdfs:subClassOf :Area .


###  http://example.org/ontologies/atkis#AdministrativeBoundary
:AdministrativeBoundary rdf:type owl:Class ;
                        rdfs:subClassOf :Administrative .


###  http://example.org/ontologies/atkis#AdministrativeCommunityArea
:AdministrativeCommunityArea rdf:type owl:Class ;
                             rdfs:subClassOf :Administrative .


###  http://example.org/ontologies/atkis#GroundCover
:AdministrativeFunction rdf:type owl:Class ;
             rdfs:comment "Administrativefunktion"@de .


###  http://example.org/ontologies/atkis#Agricultural
:Agricultural rdf:type owl:Class ;
              rdfs:subClassOf :Vegetation .


###  http://example.org/ontologies/atkis#AirTraffic
:AirTraffic rdf:type owl:Class ;
            rdfs:subClassOf :Traffic .


###  http://example.org/ontologies/atkis#AirTrafficFacility
:AirTrafficFacility rdf:type owl:Class ;
                    rdfs:subClassOf :TransportBuildingAndFacility .


###  http://example.org/ontologies/atkis#Area
:Area rdf:type owl:Class ;
      rdfs:subClassOf geo:Feature ;
      rdfs:comment "The Object theme Area, containing three \"contents\": Administrative, Protected and Geographical areas"@en .


###  http://example.org/ontologies/atkis#AreaDominatedByBuilding
:AreaDominatedByBuilding rdf:type owl:Class ;
                         rdfs:subClassOf :Settlement .


###  http://example.org/ontologies/atkis#AreaOfSpecialFunctionalCharacter
:AreaOfSpecialFunctionalCharacter rdf:type owl:Class ;
                                  rdfs:subClassOf :AreaDominatedByBuilding .


###  http://example.org/ontologies/atkis#BarrenLand
:BarrenLand rdf:type owl:Class ;
            rdfs:subClassOf :VegetationArea .


###  http://example.org/ontologies/atkis#Bog
:Bog rdf:type owl:Class ;
     rdfs:subClassOf :VegetationArea .

###  http://example.org/ontologies/atkis#EntityCondition
:BoundaryType rdf:type owl:Class ;
            rdfs:comment "Art Der Gebietsgrenze"@de .

###  http://example.org/ontologies/atkis#Building
:Building rdf:type owl:Class ;
          rdfs:subClassOf :Settlement .


###  http://example.org/ontologies/atkis#BuildingFacility
