mappingId	veg04 vegetation_characteristics
target		:VegChar/{gid} a :VegChar ; geo:hasGeometry :VegChar/f/{gid} .
source		select gid from "veg04_f"

mappingId	veg04 polygons
target		:VegChar/f/{gid} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral .
source		select gid, st_astext(geom) as wkt from "veg04_f"

