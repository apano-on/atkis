target		:SpecialFacility/l/{objid} a :SpecialFacility ; rdfs:label {nam}^^xsd:string ; :begin {beginn}^^xsd:dateTime ; :end {ende}^^xsd:dateTime ; :designFeature :WaterNavigationLock/{kon} ; :condition :EntityCondition/{zus} ; :objectTypeLabel {objart_txt} ; geo:hasDefaultGeometry :SpecialFacilitiy/geo/l/{objid} . :SpecialFacilitiy/geo/l/{objid} a geo:Geometry , sf:LineString ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
