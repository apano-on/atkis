mappingId	VerticalRelation
target		:VerticalRelation/{gid} a :VerticalRelation .
source		select gid FROM "hdu01_b"

mappingId	VerticalRelation - RailwayTraffic to TransportBuildingAndFacility
target		:RailwayTraffic/l/{objid_1} :hasDirectlyUnder :TransportBuildingAndFacility/l/{objid_2} .
source		select objid_1, objid_2 FROM "hdu01_b" WHERE ebene_1 = 'ver03_l' and ebene_2 = 'ver06_l'

mappingId	VerticalRelation - RoadTraffic to TransportBuildingAndFacility
target		:RoadTraffic/l/{objid_1} :hasDirectlyUnder :TransportBuildingAndFacility/l/{objid_2} .
source		select objid_1, objid_2 FROM "hdu01_b" WHERE ebene_1 = 'ver01_l' and ebene_2 = 'ver06_l'

mappingId	VerticalRelation - Path to TransportBuildingAndFacility
target		:Path/l/{objid_1} :hasDirectlyUnder :TransportBuildingAndFacility/l/{objid_2} .
source		select objid_1, objid_2 FROM "hdu01_b" WHERE ebene_1 = 'ver02_l' and ebene_2 = 'ver06_l'

mappingId	VerticalRelation - WaterBodyCourse multilinestring to TransportBuildingAndFacility
target		:WaterBodyCourse/l/{objid_1} :hasDirectlyUnder :TransportBuildingAndFacility/l/{objid_2} .
source		select objid_1, objid_2 FROM "hdu01_b" WHERE ebene_1 = 'gew01_l' and ebene_2 = 'ver06_l'

mappingId	VerticalRelation - WaterBodyCourse polygon to TransportBuildingAndFacility
target		:WaterBodyCourse/f/{objid_1} :hasDirectlyUnder :TransportBuildingAndFacility/l/{objid_2} .
source		select objid_1, objid_2 FROM "hdu01_b" WHERE ebene_1 = 'gew01_f' and ebene_2 = 'ver06_l'

mappingId	Relief linestring_l
target		:ReliefForm/l/{objid} a :Relief ; rdfs:label {nam} ; :function :Function/{objart}/{fkt} ; :type :SpecificType/{objart}/{art} ; :hhl {hhl}^^xsd:decimal ; :objectHeight {hho}^^xsd:decimal ; geo:hasDefaultGeometry :ReliefForm/geo/l/{objid} . :ReliefForm/geo/l/{objid} a geo:Geometry , sf:LineString ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
