target		:BuildingFacility/f/{objid} a :BuildingFacility ; rdfs:label {nam}^^xsd:string ; :begin {beginn}^^xsd:dateTime ; :end {ende}^^xsd:dateTime ; :objectHeight {hho}^^xsd:decimal ; :structureFunction :StructureFunction/{objart}/{bwf} ; :condition :EntityCondition/{zus} ; geo:hasDefaultGeometry :BuildingFacility/geo/f/{objid} . :BuildingFacility/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
