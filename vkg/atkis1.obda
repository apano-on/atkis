target		:Forestry/f/{objid} a :Forestry ; rdfs:label {nam}^^xsd:string ; :usage :Usage/{objart}/{ntz} ; geo:hasDefaultGeometry :Forestry/geo/f/{objid} . :Forestry/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
