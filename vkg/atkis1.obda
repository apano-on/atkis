source		select gid, st_astext(geom) as wkt    from "ver03_l"

mappingId	ver04
target		<http://example.org/ontologies/atkis#AirTraffic/{gid}> a <http://example.org/ontologies/atkis#AirTraffic> ; geo:hasPolygonGeometry <http://example.org/ontologies/atkis#AirTraffic/f/{gid}> . 
