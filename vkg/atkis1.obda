[PrefixDeclaration]
sf:		http://www.opengis.net/ont/sf#
geo:		http://www.opengis.net/ont/geosparql#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
wgs:		http://www.w3.org/2003/01/geo/wgs84_pos#
xml:		http://www.w3.org/XML/1998/namespace
xsd:		http://www.w3.org/2001/XMLSchema#
obda:		https://w3id.org/obda/vocabulary#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	specialFacilitiesWithinSettlement
target		<http://example.org/ontologies/atkis#SpecialFacilitiy/{b}> a <http://example.org/ontologies/atkis#SpecialFacility> ; <http://example.org/ontologies/atkis#name> {specialfacility} ; <http://example.org/ontologies/atkis#hasArea> <http://example.org/ontologies/atkis#Settlement/{c}> ; <http://example.org/ontologies/atkis#location> {area} . 
source		SELECT s.gid as b, s.nam AS specialfacility, a.gid as c, a.nam as area,
			st_astext(s.geom) as point, st_astext(a.geom) as polygon
			FROM "sie04_f" s
			JOIN "sie01_f" a
			ON ST_Intersects(s.geom, a.geom);

mappingId	settlement
target		<http://example.org/ontologies/atkis#Settlement/{gid}> a <http://example.org/ontologies/atkis#Settlement> ; <http://example.org/ontologies/atkis#location> {nam} ; <http://example.org/ontologies/atkis#geometry> {wkt}^^geo:wktLiteral ; <http://example.org/ontologies/atkis#rgs> {rgs} ; <http://example.org/ontologies/atkis#objectId> {objid} . 
source		select *, st_astext(geom) as wkt from "sie01_f"

mappingId	area
target		<http://example.org/ontologies/atkis#Area/{gid}> a <http://example.org/ontologies/atkis#Area> ; <http://example.org/ontologies/atkis#name> {nam} ; <http://example.org/ontologies/atkis#geometry> {wkt} . 
source		select *,st_astext(geom) as wkt from "sie01_f"

mappingId	long lat of area
target		<http://example.org/ontologies/atkis#Area/{bez_lan}> a <http://example.org/ontologies/atkis#Area> ; <http://example.org/ontologies/atkis#name> {bez_lan} ; wgs:long {longitude} ; wgs:lat {latitude} ; <http://example.org/ontologies/atkis#bound> {boundary}^^geo:wktLiteral . 
source		select * from "area_xy2"

mappingId	relief
target		<http://example.org/ontologies/atkis#Relief/{id}> a <http://example.org/ontologies/atkis#Relief> . 
source		select * from "rel01_l"

mappingId	vegetation
target		<http://example.org/ontologies/atkis#Vegetation/{gid}> a <http://example.org/ontologies/atkis#Vegetation> ; <http://example.org/ontologies/atkis#name> {nam} ; <http://example.org/ontologies/atkis#intersectsWith> <http://example.org/ontologies/atkis#Area/{gid}> . 
source		select *, st_astext(geom) as polygon from "veg01_f"

mappingId	vertical relation
target		<http://example.org/ontologies/atkis#Ver/{gid}> a <http://example.org/ontologies/atkis#VerticalRelation> . 
source		select * from "hdu01_b"

mappingId	water bodies
target		<http://example.org/ontologies/atkis#WaterBody/{id}> a <http://example.org/ontologies/atkis#WaterBody> . 
source		select * from "gew01_f"

mappingId	sie01 points
target		<http://example.org/ontologies/atkis#Settlement/point/{gid}> a <http://example.org/ontologies/atkis#Settlement> ; <http://example.org/ontologies/atkis#name> {nam}^^xsd:string . 
source		select gid, nam, st_astext(geom) as wkt from "sie01_p"

mappingId	sie01 polygons
target		<http://example.org/ontologies/atkis#Settlement/polygon/{gid}> a <http://example.org/ontologies/atkis#Settlement> ; <http://example.org/ontologies/atkis#name> {nam}^^xsd:string . 
source		select gid, nam, st_astext(geom) as wkt from "sie01_f"

mappingId	sie02 areas dom.buildings
