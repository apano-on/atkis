[MappingDeclaration] @collection [[
mappingId	Area for linestring_l
target		:Administrative/l/{objid} a :Administrative ; :countryCode {land}^^xsd:string ; :objectType {objart}^^xsd:string ; :begin {beginn}^^xsd:dateTime ; :condition :EntityCondition/{zus} ; :boundaryType :BoundaryType/{agz_1} ; geo:hasDefaultGeometry :Administrative/geo/l/{objid} . :Administrative/geo/l/{objid} a geo:Geometry , sf:LineString ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
source		select objid, st_astext(geom) as wkt, objart, beginn, land, zus, agz_1 from "geb01_l"

mappingId	Area for linestring_l - agz2
target		:Administrative/l/{objid} :boundaryType :BoundaryType/{agz_2} .
source		select objid, agz_2 from "geb01_l" where agz_2 is not null

mappingId	Area for linestring_l - agz3
target		:Administrative/l/{objid} :boundaryType :BoundaryType/{agz_3} .
source		select objid, agz_3 from "geb01_l" where agz_3 is not null

mappingId	Area for linestring_l - agz4
target		:Administrative/l/{objid} :boundaryType :BoundaryType/{agz_4} .
source		select objid, agz_4 from "geb01_l" where agz_4 is not null

mappingId	Area for linestring_l - agz5
target		:Administrative/l/{objid} :boundaryType :BoundaryType/{agz_5} .
source		select objid, agz_5 from "geb01_l" where agz_5 is not null

mappingId	Area for linestring_l - agz6
target		:Administrative/l/{objid} :boundaryType :BoundaryType/{agz_6} .
source		select objid, agz_6 from "geb01_l" where agz_6 is not null

mappingId	administrative polygon
target		:Administrative/f/{objid} a :Administrative ; rdfs:label {bez_lan}^^xsd:string ; :countryDesignation {bez_nat}^^xsd:string ; :stateDesignation {bez_lan}^^xsd:string ; :districtDesignation {bez_rbz}^^xsd:string ; :circleDesignation {bez_krs}^^xsd:string ; :townshipDesignation {bez_vwg}^^xsd:string ; :municipalityDesignation {bez_gem}^^xsd:string ; :catalogueEntryAbbreviation {sch}^^xsd:string ; :administrativeFunction :AdministrativeFunction/{adm} ; geo:hasDefaultGeometry :Administrative/geo/f/{objid} . :Administrative/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
source		select objid, adm, sch, bez_nat, bez_lan, bez_rbz, bez_krs, bez_vwg, bez_gem, st_astext(geom) as wkt from "geb01_f"

mappingId	Area, geographical areas polygon (f)
target		:Geographical/f/{objid} a :Geographical ; rdfs:label {nam}^^xsd:string ; :landscapeType :LandscapeType/{ltp} ; geo:hasDefaultGeometry :Geographical/geo/f/{objid} . :Geographical/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
source		select objid, nam, ltp, st_astext(geom) as wkt from "geb02_f"

mappingId	Protected areas polygons_f
target		:Protected/f/{objid} a :Protected ; rdfs:label {nam}^^xsd:string ; :specificationType :SpecificationType/{adf} ; :numberOfProtectedArea {sgn}^^xsd:integer ; geo:hasDefaultGeometry :Protected/geo/f/{objid} . :Protected/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
source		select objid, nam, adf, sgn, st_astext(geom) as wkt from "geb03_f"

mappingId	WaterBody polygon_f
target		:WaterBodyCourse/f/{objid} a :WaterBodyCourse ; rdfs:label {nam}^^xsd:string ; :function :Function/{objart}/{fkt} ; :watercourseCode {gwk}^^xsd:string ; :seaCode {skz}^^xsd:string ; :hydrologicalFeature {hyd_boolean}^^xsd:boolean ; :waterLevel {wsg}^^xsd:decimal ; :navigationCategory :NavigationCategory/{sfk} ; :condition :EntityCondition/{zus} ; :objectTypeLabel {objart_txt} ; geo:hasDefaultGeometry :WaterBodyCourse/geo/f/{objid} . :WaterBodyCourse/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral ; :entityClassification :EntityClassification/{objart}/{wdm} .
source		select objid, nam, objart, objart_txt, fkt, sfk, gwk, skz, wdm, wsg, zus, st_astext(geom) as wkt, CASE WHEN (hyd='2000') THEN TRUE ELSE FALSE END AS hyd_boolean from "gew01_f"

mappingId	WaterBody linestrings_l
target		:WaterBodyCourse/l/{objid} a :WaterBodyCourse ; rdfs:label {nam}^^xsd:string ; :function :Function/{objart}/{fkt} ; :watercourseCode {gwk}^^xsd:string ; :widthOfWaterbody {brg}^^xsd:integer ; :flowDirection {flr}^^xsd:boolean ; :navigationCategory :NavigationCategory/{sfk} ; :condition :EntityCondition/{zus} ; geo:hasDefaultGeometry :WaterBodyCourse/geo/l/{objid} . :WaterBodyCourse/geo/l/{objid} a geo:Geometry , sf:LineString ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral ; :entityClassification :EntityClassification/{objart}/{wdm} .
source		select objid, nam, objart, sfk, fkt, flr, gwk, brg, wdm, zus, st_astext(geom) as wkt from "gew01_l"

mappingId	WaterBodyCharacteristic polygon_f
target		:WaterBodyCharacteristic/f/{objid} a :WaterBodyCharacteristic ; rdfs:label {nam}^^xsd:string ; :type :SpecificType/{objart}/{art} ; geo:hasDefaultGeometry :WaterBodyCharacteristic/geo/f/{objid} . :WaterBodyCharacteristic/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
source		select objid, nam, art, objart, st_astext(geom) as wkt from "gew02_f"

mappingId	WaterBodyCharacteristic point_p
target		:WaterBodyCharacteristic/p/{objid} a :WaterBodyCharacteristic ; rdfs:label {nam}^^xsd:string ; :type :SpecificType/{objart}/{art} ; geo:hasDefaultGeometry :WaterBodyCharacteristic/geo/p/{objid} . :WaterBodyCharacteristic/geo/p/{objid} a geo:Geometry , sf:Point ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
source		select objid, nam, art, objart, st_astext(geom) as wkt from "gew02_p"

mappingId	WaterLineAxis linestring_l
target		:WaterLineAxis/l/{objid} a :WaterLineAxis ; rdfs:label {nam}^^xsd:string ; :watercourseCode {gwk}^^xsd:string ; :flowDirection {flr}^^xsd:boolean ; :waterStationingAxis :WaterStationingAxis/{aga} ; :navigationCategory :NavigationCategory/{sfk} ;  geo:hasDefaultGeometry :WaterLineAxis/geo/l/{objid} . :WaterLineAxis/geo/l/{objid} a geo:Geometry , sf:LineString ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
