mappingId	Settlement, named location polygon_f
target		:Location/f/{objid} a :Location ; rdfs:label {nam}^^xsd:string ; :regionalLanguage {rgs}^^xsd:string ; :begin {beginn}^^xsd:dateTime ; :end {ende}^^xsd:dateTime ; :countryCode {land}^^xsd:string ; :hasDirectlyUnderExists {hdux_boolean}^^xsd:boolean ; geo:hasDefaultGeometry :Location/geo/f/{objid} . :Location/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
source		select objid, nam, rgs, beginn, ende, land, CASE WHEN (hdu_x=1) THEN TRUE ELSE FALSE END AS hdux_boolean, st_astext(geom) as wkt from "sie01_f"

mappingId	Settlement, named location point_p
target		:Location/p/{objid} a :Location ; rdfs:label {nam}^^xsd:string ; :regionalLanguage {rgs}^^xsd:string ; :begin {beginn}^^xsd:dateTime ; :end {ende}^^xsd:dateTime ; :countryCode {land}^^xsd:string ; :hasDirectlyUnderExists {hdux_boolean}^^xsd:boolean ; geo:hasDefaultGeometry :Location/geo/p/{objid} . :Location/geo/p/{objid} a geo:Geometry , sf:Point ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
