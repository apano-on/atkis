target		:Protected/f/{gid} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral .
source		select gid, st_astext(geom) as wkt from "geb03_f"

mappingId	rel01 relief forms linestring
target		:Relief/{id} a :Relief ; geo:hasLinestringGeometry :Protected/l/{id} .
source		select id from "rel01_l"

mappingId	rel01 linestring
target		:Relief/l/{id} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral .
source		select id, st_astext(geom) as wkt from "rel01_l"

mappingId	rel01 relief forms points
target		:Relief/{gid} a :Relief ; geo:hasPointGeometry :Relief/p/{gid} .
source		select gid from "rel01_p"

mappingId	rel01 points
target		:Relief/p/{gid} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral .
