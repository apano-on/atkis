target		:WaterBody/{id} a :WaterBody ; geo:hasPolygonGeometry :WaterBody/f/{id} ; geo:hasLinestringGeometry :WaterBody/l/{gid} .
source		select gew01_f.id, gew01_l.gid from "gew01_f", "gew01_l"

mappingId	gew01 polygons
target		:WaterBody/f/{id} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral .
source		select id, st_astext(geom) as wkt from "gew01_f"

mappingId	gew01 linestring
target		:WaterBody/l/{gid} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral .
source		select gid, st_astext(geom) as wkt from "gew01_l"

