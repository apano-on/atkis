mappingId	ver06 points
target		:Transport_b_f/p/{id} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral ; rdfs:label {nam}^^xsd:string . 
source		select id, st_astext(geom) as wkt, nam from "ver06_p"

mappingId	veg01 vegetation
target		:Agricultural/{gid} a :Agricultural ; geo:hasGeometry :Agricultural/f/{gid} .
source		select gid from "veg01_f"

mappingId	veg01 polygons
target		:Agricultural/f/{gid} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral .
source		select gid, st_astext(geom) as wkt from "veg01_f"

mappingId	veg02 Forestry use
target		:Forestry/{gid} a :Forestry ; geo:hasGeometry :Forestry/f/{gid} .
source		select gid from "veg02_f"

mappingId	veg02 polygons
target		:Forestry/f/{gid} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral .
source		select gid, st_astext(geom) as wkt from "veg02_f"

mappingId	veg03 vegetation
target		:Vegetation/{gid} a :Vegetation ; geo:hasGeometry :Vegetation/f/{gid} .
