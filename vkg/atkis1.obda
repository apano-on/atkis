mappingId	BuildingFunction
target		:BuildingFunction/p/{code} a :BuildingFunction ; rdfs:label {name_de}@de ; rdfs:comment {definition_de}@de .
source		select code, name_de, definition_de from "gebaeudefunktion"

mappingId	BuildingFunction - English
target		:BuildingFunction/p/{code} rdfs:label {name_en}@en .
source		select code, name_en from "gebaeudefunktion"

