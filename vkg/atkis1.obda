target		:SpecialFacility/f/{objid} a :SpecialFacility ; rdfs:label {nam}^^xsd:string ; :begin {beginn}^^xsd:dateTime ; :end {ende}^^xsd:dateTime ; :objectTypeLabel {objart_txt} ; geo:hasDefaultGeometry :SpecialFacilitiy/geo/f/{objid} . :SpecialFacilitiy/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
