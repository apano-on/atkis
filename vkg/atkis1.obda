target		:Transport_b_f/f/{gid} a geo:geometry ; geo:asWKT {wkt}^^geo:wktLiteral ; rdfs:label {nam}^^xsd:string . 
source		select gid, st_astext(geom) as wkt, nam from "ver06_f"

