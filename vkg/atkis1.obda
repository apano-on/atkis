mappingId	vegetation_characteristics polygon_f
target		:VegetationCharacteristic/f/{objid} a :VegetationCharacteristic ; rdfs:label {nam}^^xsd:string ; :groundCover :GroundCover/{bws} ; :condition :EntityCondition/{zus} ; geo:hasDefaultGeometry :VegetationCharacteristic/geo/f/{objid} . :VegetationCharacteristic/geo/f/{objid} a geo:Geometry , sf:MultiPolygon ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral .
