mappingId	SpecialFacility point_p
target		:SpecialFacility/p/{objid} a :SpecialFacility ; rdfs:label {nam}^^xsd:string ; :begin {beginn}^^xsd:dateTime ; :end {ende}^^xsd:dateTime ; :condition :EntityCondition/{zus} ; :objectTypeLabel {objart_txt} ; geo:hasDefaultGeometry :SpecialFacilitiy/geo/p/{objid} . :SpecialFacilitiy/geo/p/{objid} a geo:Geometry , sf:Point ; geo:asWKT "<http://www.opengis.net/def/crs/EPSG/0/31467> {wkt}"^^geo:wktLiteral ; :objectTypeLabel {objart_txt} .
