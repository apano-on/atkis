source		select gid, nam, st_astext(geom) as wkt from "sie02_f"

mappingId	sie03 buildings for sport, historical stuff or indrustry
target		<http://example.org/ontologies/atkis#Settlement/sie03/{id}> a <http://example.org/ontologies/atkis#Settlement> ; <http://example.org/ontologies/atkis#name> {nam} ; <http://example.org/ontologies/atkis#objart_txt> {objart_txt} ; rdf:geometry {wkt} . 
source		select id, nam, objart_txt, st_astext(geom) as wkt from "sie03_p"

mappingId	sie04_f
target		<http://example.org/ontologies/atkis#Settlement/sie04/f/{gid}> a <http://example.org/ontologies/atkis#Settlement> ; <http://example.org/ontologies/atkis#name> {nam} ; <http://example.org/ontologies/atkis#sie04_polygon> {wkt} . 
source		select gid, nam, st_astext(geom) as wkt from "sie04_f"

mappingId	sie04_l
target		<http://example.org/ontologies/atkis#Settlement/sie04/l/{gid}> a <http://example.org/ontologies/atkis#Settlement> ; <http://example.org/ontologies/atkis#name> {nam}^^xsd:string ; <http://example.org/ontologies/atkis#sie04Linestring> {wkt}^^geo:wktLiteral . 
